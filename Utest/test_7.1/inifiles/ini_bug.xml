<JuPedSim xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" project="JPS-Project" version="0.5" xsi:noNamespaceSchemaLocation="ini.xsd">
 
  <seed>1</seed>
  <max_sim_time>200</max_sim_time>
  <numCPU>4</numCPU>

  
  <geometry>../geometry.xml</geometry>
  
  <trajectories embed_mesh="false" format="xml-plain" fps="33">
    <file location="../trajectories/traj_operational_model_id_1.xml" />
  </trajectories>
  
  <logfile>log</logfile>

  
  <traffic_constraints>
    
    <rooms>
      <room room_id="0" state="good" />
    </rooms>
    
    <doors>
      <door caption="" state="open" trans_id="0" />
    </doors>
  </traffic_constraints>
  <routing>
    <goals>
      <goal caption="goal" final="true" id="0">
        <polygon>
          <vertex px="14" py="4" />
          <vertex px="14" py="0" />
          <vertex px="13" py="0" />
          <vertex px="13" py="4" />
          <vertex px="14" py="4" />
        </polygon>
      </goal>
    </goals>
  </routing>

  
  <agents operational_model_id="2">
    <agents_distribution>
      <group agent_parameter_id="0" goal_id="-1" group_id="0" number="300" room_id="0" router_id="1" subroom_id="0" />	
    </agents_distribution>
    
    <agents_sources />
  </agents>


<operational_models>
    <model description="gcfm" operational_model_id="1">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.01</stepsize>
        <exitCrossingStrategy>3</exitCrossingStrategy>
        <linkedcells cell_size="2.8" enabled="true" />
        <force_ped dist_max="3" disteff_max="2" interpolation_width="0.1" nu="0.2" />
        <force_wall dist_max="3" disteff_max="2" interpolation_width="0.1" nu="0.2" />
      </model_parameters>
      <agent_parameters agent_parameter_id="0">
        <v0 mu="1" sigma="0.0" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.5" sigma="0.001" />
      </agent_parameters>
    </model>

    <model description="gompertz" operational_model_id="2">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.01</stepsize>
        <exitCrossingStrategy>3</exitCrossingStrategy>
        <linkedcells cell_size="2.8" enabled="true" />
        <force_ped b="0.25" c="3.0" nu="2" />
        <force_wall b="0.70" c="3.0" nu="2" />
      </model_parameters>
      <agent_parameters agent_parameter_id="0">
        <v0 mu="1" sigma="0.0" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.5" sigma="0.001" />
      </agent_parameters>
    </model>
  </operational_models>

  <route_choice_models>
    <router description="quickest" router_id="1">
      <parameters>
        
      </parameters>
    </router>
    <router description="global_shortest" router_id="2">
      <parameters>
        
      </parameters>
    </router>

  </route_choice_models>

</JuPedSim>
