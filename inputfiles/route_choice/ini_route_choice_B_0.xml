<?xml version="1.0" encoding="UTF-8" ?> 
<JuPedSim project="JPS-Project" version="0.7" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://xsd.jupedsim.org/0.7/jps_ini_core.xsd"> 
    <seed>12542</seed> 
    <geometry>geo_route_choice_B_0.xml</geometry> 
    <show_statistics>true</show_statistics> 
    <trajectories format="xml-plain"  fps="16"> 
        <file location="traj_route_choice_B_0.xml" /> 
    </trajectories> 
<!--     <logfile>log.txt</logfile>  -->
    <traffic_constraints> 
        <doors> 
            <door trans_id="0" caption="left" state="open" /> 
            <door trans_id="1" caption="right" state="open" /> 
            <door trans_id="2" caption="up" state="open" /> 
            <door trans_id="3" caption="down" state="open" /> 
            <door trans_id="4" caption="leave_left" state="open" /> 
            <door trans_id="5" caption="leave_right" state="open" /> 
            <door trans_id="6" caption="leave_up" state="open" /> 
            <door trans_id="7" caption="leave_down" state="open" /> 
        </doors> 
    </traffic_constraints> 
    <agents operational_model_id="2"> 
        <agents_distribution> 
            <group group_id="1" router_id="1" agent_parameter_id="1" room_id="0" subroom_id="0" number="11" x_min="2.4" x_max="4.4" y_min="-3.5" y_max="-1.5" /> 
        </agents_distribution> 
    </agents> 
    <operational_models> 
        <model operational_model_id="2" description="gompertz"> 
            <model_parameters> 
                <solver>euler</solver> 
                <stepsize>0.01</stepsize> 
                <exit_crossing_strategy>3</exit_crossing_strategy> 
                <linkedcells enabled="true" cell_size="3.9" /> 
                <force_ped nu="3"  b="0.25" c="3.0"/> 
                <force_wall nu="10" b="0.70" c="3.0"/> 
            </model_parameters> 
            <agent_parameters agent_parameter_id="1"> 
                <v0 mu="1.55" sigma="0.18" /> 
                <bmax mu="0.25" sigma="0.001" /> 
                <bmin mu="0.20" sigma="0.001" /> 
                <amin mu="0.18" sigma="0.001" /> 
                <tau mu="0.5" sigma="0.001" /> 
                <atau mu="0.5" sigma="0.001" /> 
            </agent_parameters> 
        </model> 
    </operational_models> 
    <route_choice_models> 
        <router router_id="1" description="quickest"> 
        <parameters cba_gain="0.15" reference_peds_selection="single" congestion_ratio="0.8"
        queue_vel_escaping_jam="0.2" queue_vel_new_room="0.7" visibility_obstruction="4"/> 
        </router> 
    </route_choice_models> 
</JuPedSim> 
