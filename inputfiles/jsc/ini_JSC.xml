					<vertex px="57.0" py="53.0" />
					<vertex px="50.0" py="53.0" />
					<vertex px="50.0" py="48.0" />
				</polygon>
			</goal>
		</goals>

		<!-- in the form goal_id1:goal_id2:goal_id3:..:goal_idn -->
