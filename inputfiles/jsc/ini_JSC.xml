			<door trans_id="1" caption="" state="open" />
			<door trans_id="4" caption="" state="close" />
		</doors>
	</traffic_constraints>

	<routing>
		<goals>
			<goal id="0" final="false" caption="goal 1">
				<polygon>
					<vertex px="-7.0" py="25.0" />
