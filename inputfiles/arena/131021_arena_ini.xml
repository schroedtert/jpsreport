					<vertex px="115.0" py="95.0" />
				</polygon>
			</goal>
			
			<goal id="3" final="true" caption="goal 3">
				<polygon>
					<vertex px="115.0" py="-95.0" />
					<vertex px="100.0" py="-95.0" />
					<vertex px="100.0" py="-80.0" />
					<vertex px="115.0" py="-80.0" />
					<vertex px="115.0" py="-95.0" />
				</polygon>
			</goal>
		</goals>
	</routing>

