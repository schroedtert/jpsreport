<JuPedSim xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" project="JPS-Project" version="0.5" xsi:noNamespaceSchemaLocation="../../xsd/jps_ini_core.xsd">

  
  <seed>1254</seed>
  <max_sim_time>200</max_sim_time>
  <num_threads>8</num_threads>
  
  <geometry>geometry_Ax_0.8.xml</geometry>
  
  <trajectories format="xml-plain" fps="8">
    <file location="traj_0.8.xml" />
    
  </trajectories>
  
  


  
  <traffic_constraints>
    <doors>
      
    </doors>
  </traffic_constraints>

  <routing>
    
  </routing>

  
  <agents operational_model_id="1">
    <agents_distribution>
      <group agent_parameter_id="1" goal_id="-1" group_id="0" number="1" room_id="0" router_id="1" start_x="-1" start_y="1" subroom_id="0" />
      
    </agents_distribution>
  </agents>

  
  
  <operational_models>
    <model description="gcfm" operational_model_id="1">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.01</stepsize>
        <exit_crossing_strategy>4</exit_crossing_strategy>
        <linkedcells cell_size="2.2" enabled="true" />
        <force_ped dist_max="3" disteff_max="2" interpolation_width="0.1" nu="0.3" />
        <force_wall dist_max="3" disteff_max="2" interpolation_width="0.1" nu="0.2" />
      </model_parameters>
      <agent_parameters agent_parameter_id="1">
        <v0 mu="1.5" sigma="0.0" />
        <v0_upstairs mu="0.5" sigma="0.0" />
        <v0_downstairs mu="0.5" sigma="0.0" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.5" sigma="0.001" />
      </agent_parameters>
      <agent_parameters agent_parameter_id="2">
        <v0 mu="1.3" sigma="0.0" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.5" sigma="0.001" />
      </agent_parameters>
    </model>

    <model description="gompertz" operational_model_id="2">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.01</stepsize>
        <exit_crossing_strategy>3</exit_crossing_strategy>
        <linkedcells cell_size="2.2" enabled="true" />
        <force_ped b="0.25" c="3.0" nu="3" />
        <force_wall b="0.70" c="3.0" nu="10" />
      </model_parameters>
      <agent_parameters agent_parameter_id="1">
        <v0 mu="0.5" sigma="0.05" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.5" sigma="0.001" />
      </agent_parameters>
      <agent_parameters agent_parameter_id="2">
        <v0 mu="0" sigma="0.0" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.5" sigma="0.001" />
      </agent_parameters>
    </model>
  </operational_models>

  <route_choice_models>
    <router description="global_shortest" router_id="1">
      <parameters>
	
      </parameters>
    </router>

  </route_choice_models>

</JuPedSim>
