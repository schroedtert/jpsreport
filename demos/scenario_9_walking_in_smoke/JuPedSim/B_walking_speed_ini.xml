<?xml version="1.0" encoding="UTF-8" ?>

<JuPedSim project="JPS-Project" version="0.5"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="../../xsd/jps_ini_core.xsd">

  <!-- seed used for initialising random generator -->
  <seed>3011</seed>
  <max_sim_time>600</max_sim_time>
  <!-- geometry file -->
  <geometry>B_walking_speed_geo.xml</geometry>
  <!-- trajectories file and format -->
  <trajectories format="xml-plain" fps="24">
    <file location="B_walking_speed_traj.xml" />
    <!--<socket hostname="127.0.0.1" port="8989"/> -->
  </trajectories>
  <!-- where to store the logs -->
  <!--<logfile>log.txt</logfile> -->


  <!-- traffic information: e.g closed doors -->
  <traffic_constraints>
  </traffic_constraints>

  <routing>
  </routing>

  <!-- configuration of fire simulation input -->
  <JPSfire>
  	<A_smoke_sensor/>
   	<B_walking_speed extinction_grids="../FDS/2_extinction_grids/EXTINCTION_COEFFICIENT/" update_time="10.0" final_time="120.0" study="Jin1978" irritant="false"/>
   	<C_toxicity_analysis/>
  </JPSfire>

  <!--persons information and distribution -->
  <agents operational_model_id="1">
    <agents_distribution>

      <group group_id="0" agent_parameter_id="1" room_id="1" subroom_id="0" number="1" goal_id="-1" router_id="1" pre_movement_mean="10" pre_movement_sigma="0" />

    </agents_distribution>
  </agents>


  <!-- These parameters may be overwritten -->
      <!-- These parameters may be overwritten -->
 <operational_models>
    <model operational_model_id="1" description="gcfm">
      <model_parameters>
        <solver>euler</solver>
        <stepsize>0.01</stepsize>
        <exit_crossing_strategy>1</exit_crossing_strategy>
        <linkedcells enabled="true" cell_size="2.2" />
        <force_ped nu="0.3" dist_max="3" disteff_max="2" interpolation_width="0.1" />
        <force_wall nu="0.2" dist_max="3" disteff_max="2" interpolation_width="0.1" />
      </model_parameters>
      <agent_parameters agent_parameter_id="1">
        <v0 mu="1.4" sigma="0.0" />
        <bmax mu="0.25" sigma="0.001" />
        <bmin mu="0.20" sigma="0.001" />
        <amin mu="0.18" sigma="0.001" />
        <tau mu="0.5" sigma="0.001" />
        <atau mu="0.5" sigma="0.001" />
      </agent_parameters>
    </model>
  </operational_models>

  <route_choice_models>
    <router router_id="1" description="global_shortest">
      <parameters>
      	<navigation_lines file="B_walking_speed_routing.xml" />
      </parameters>
    </router>
  </route_choice_models>

</JuPedSim>
